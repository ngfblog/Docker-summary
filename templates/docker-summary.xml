<Container>
  <Name>docker-summary</Name>
  <Repository>docker-summary-server</Repository>
  <Network>bridge</Network>
  <Shell>bash</Shell>
  <Privileged>true</Privileged>
  <Environment>
    <Variable>
      <Name>TZ</Name>
      <Value>Asia/Jerusalem</Value>
    </Variable>
  </Environment>
  <Config>
    <Data>
      <Name>docker.sock</Name>
      <Source>/var/run/docker.sock</Source>
      <Target>/var/run/docker.sock</Target>
      <Mode>rw</Mode>
    </Data>
  </Config>
  <WebUI>http://[IP]:[PORT:8090]/view</WebUI>
</Container>
